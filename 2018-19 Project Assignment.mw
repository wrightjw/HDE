<Group view="code" hide-input="false" labelreference="L5287" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">critical_points_implicit := solve({rhs(sys9[1])=0,rhs(sys9[2])=0,rhs(sys9[3])=0},{x(t),y(t),z(t)}): #Solving for critical points by setting the right-hand side of each equation to zero.
critical_points_explicit := map(allvalues,{critical_points_implicit}): #Finding explicit solutions
CP1_eval := eval(critical_points_explicit[1]): #Evaluating first critical point
CP2_eval := eval(critical_points_explicit[2]): #Evaluating second critical point

CP1 := eval([x(t),y(t),z(t)],CP1_eval);
CP2 := eval([x(t),y(t),z(t)],CP2_eval);

</Text-field>
</Input>
